{
    "name": "seatplus/api",
    "description": "Package to consume seatplus data trough an api",
    "keywords": [
        "seatplus",
        "laravel",
        "api"
    ],
    "homepage": "https://github.com/seatplus/api",
    "license": "MIT",
    "authors": [
        {
            "name": "Felix Huber",
            "email": "felix.a.huber@gmx.net",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "laravel/sanctum": "^2.11",
        "seatplus/web": "*"
    },
    "require-dev": {
        "nunomaduro/collision": "^5.3",
        "orchestra/testbench": "^6.15",
        "pestphp/pest-plugin-laravel": "^1.1",
        "pestphp/pest-plugin-parallel": "^0.2.0"
    },
    "autoload": {
        "psr-4": {
            "Seatplus\\Api\\": "src",
            "Seatplus\\Api\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Seatplus\\Api\\Tests\\": "tests"
        }
    },
    "scripts": {
      "test": "./vendor/bin/pest",
      "test-coverage": "XDEBUG_MODE=coverage ./vendor/bin/pest --coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Seatplus\\Api\\ApiServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
